{
  "name": "extas/operations",
  "installer_version": 3,
  "repositories": [
    {
      "name": "operations",
      "scope": "extas",
      "pk": "name",
      "class": "extas\\components\\operations\\Operation",
      "aliases": ["operations"]
    },
    {
      "name": "operations_samples",
      "scope": "extas",
      "pk": "name",
      "class": "extas\\components\\operations\\OperationSample",
      "aliases": ["operationsSamples"]
    }
  ],
  "plugins_install": [
    {
      "repository": "operations",
      "name": "operation",
      "section": "operations"
    },
    {
      "repository": "operationsSamples",
      "name": "operation sample",
      "section": "operations_samples"
    }
  ]
}